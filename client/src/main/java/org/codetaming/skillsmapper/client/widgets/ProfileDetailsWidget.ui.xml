<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
    <b:Container fluid="true">
        <b:Row>
            <b:Column size="MD_8">
                <b:PageHeader ui:field="name"/>
                <b:Heading size="H3" ui:field="titlesHeader"/>
                <b:Heading size="H4" ui:field="groupsHeader"/>
            </b:Column>
            <b:Column size="MD_4">
                <b:Image type="THUMBNAIL" ui:field="photo"></b:Image>
            </b:Column>
        </b:Row>
    </b:Container>
</ui:UiBinder>